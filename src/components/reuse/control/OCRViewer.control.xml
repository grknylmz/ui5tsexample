
        
        sap.ui.require.preload({"sap/bdp/components/reuse/control/OCRViewer.control.xml": "<FragmentDefinition\n        xmlns=\"sap.ui.core\"\n        xmlns:m=\"sap.m\"\n        xmlns:l=\"sap.ui.layout\"\n\t\txmlns:f=\"sap.f\"\n\t\txmlns:card=\"sap.f.cards\"\n        xmlns:form=\"sap.ui.layout.form\"\n        xmlns:customdata=\"http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1\"\n>\n    <m:VBox width=\"100%\" height=\"100%\" justifyContent=\"Center\">\n        <m:items>\n            <m:OverflowToolbar>\n                <m:Button text=\"\"\n                          id=\"btnFirstPage\"\n                          type=\"Transparent\"\n                          icon=\"sap-icon://sys-first-page\"\n                          enabled=\"{= ${/currentPage}.pageNumber !== 1 }\"\n                          tooltip=\"{i18n>lb_tp_first_page}\"\n                          press=\"onFirstPagePress\"/>\n                <m:Button text=\"\"\n                          id=\"btnPreviousPage\"\n                          type=\"Transparent\"\n                          icon=\"sap-icon://sys-prev-page\"\n                          tooltip=\"{i18n>lb_tp_prev_page}\"\n                          enabled=\"{= ${/currentPage}.pageNumber !== 1 }\"\n                          press=\"onPrevPagePress\"/>\n                <m:Select id=\"pageSelect\" change=\"onPageSelectChange\"\n                          selectedKey=\"{= ${/currentPage}.pageNumber }\" items=\"{ path: '/pages' }\">\n                    <Item key=\"{pageNumber}\" text=\"{pageNumber}\" />\n                </m:Select>\n                <m:Label text=\"/ {= ${/pages}.length }\"/>\n                <m:Button text=\"\"\n                          id=\"btnNextPage\"\n                          type=\"Transparent\"\n                          icon=\"sap-icon://sys-next-page\"\n                          enabled=\"{= ${/currentPage}.pageNumber !== ${/pages}.length }\"\n                          tooltip=\"{i18n>lb_tp_next_page}\"\n                          press=\"onNextPagePress\"/>\n                <m:Button text=\"\"\n                          id=\"btnLastPage\"\n                          type=\"Transparent\"\n                          icon=\"sap-icon://sys-last-page\"\n                          enabled=\"{= ${/currentPage}.pageNumber !== ${/pages}.length }\"\n                          tooltip=\"{i18n>lb_tp_last_page}\"\n                          press=\"onLastPagePress\"/>\n\n                <m:ToolbarSpacer/>\n                <m:Button text=\"\"\n                          id=\"btnZoomIn\"\n                          type=\"Transparent\"\n                          icon=\"sap-icon://zoom-in\"\n                          tooltip=\"{i18n>lb_tp_zoom_in}\"\n                          enabled=\"{= ${/zoomPercentage} !== 400}\"\n                          press=\"onZoomIn\"/>\n                <m:Button text=\"\"\n                          id=\"btnZoomOut\"\n                          type=\"Transparent\"\n                          icon=\"sap-icon://zoom-out\"\n                          tooltip=\"{i18n>lb_tp_zoom_out}\"\n                          enabled=\"{= ${/zoomPercentage} !== 50}\"\n                          press=\"onZoomOut\"/>\n                <m:Button text=\"Fit width\"\n                          id=\"btnFitWidth\"\n                          type=\"Transparent\"\n                          icon=\"\"\n                          tooltip=\"{i18n>lb_tp_fit_width}\"\n                          press=\"onFitWidth\"/>\n                \n                <m:ToolbarSpacer visible=\"{= ${/showAnnotations} &amp;&amp; ${/isAnnotationUI}}\"/>\n                <Icon id=\"selectorHelp\"\n                          src=\"sap-icon://sys-help-2\"\n                          color=\"Default\"\n                          decorative=\"false\"\n                          tooltip=\"See how to annotate sample document\"\n                          press=\"onSelectorHelp\" \n                          visible=\"{= ${/showAnnotations} &amp;&amp; ${/isAnnotationUI}}\"\n                          />\n            </m:OverflowToolbar>\n\n\n            <m:ScrollContainer id=\"pagesContainer\" class=\"previewContainer\" width=\"100%\">\n                <l:VerticalLayout id=\"imageLayout\" binding=\"{/currentPage}\" visible=\"{= ${/currentPage} !== undefined &amp;&amp; ${/currentPage} !== null }\" width=\"{= ${/zoomPercentage}+'%'}\">\n                    <m:Image alt=\"{= 'page-'+${pageNumber}}\"\n                             id=\"ocrImage\"\n                             src=\"{= ${/documentUrl}+'/pages/'+${pageNumber}+'?clientId='+${/clientId}}\"\n                             load=\".onImageLoad\"\n                             width=\"100%\"\n                    />\n                    <HTML sanitizeContent=\"true\" id=\"headerFieldDivs\" preferDOM=\"false\" content=\"{parts: ['/imageHeight', '/imageWidth', 'headerFields'], formatter:'.createHeaderFieldsDivs'}\" />\n                    <HTML sanitizeContent=\"true\" id=\"lineItemDivs\" preferDOM=\"false\" content=\"{parts: ['/imageHeight', '/imageWidth', 'lineItems'], formatter:'.createLineItemsDivs'}\" />\n                    <HTML sanitizeContent=\"true\" id=\"annotationDivs\" preferDOM=\"false\" content=\"{parts: ['/imageHeight', '/imageWidth', '/showAnnotations', '/annotations', '/selectedBox'], formatter:'.createAnnotationsDivs'}\" />\n                    <HTML sanitizeContent=\"true\" id=\"selectedBoxDivs\" preferDOM=\"false\" content=\"{parts: ['/imageHeight', '/imageWidth', '/selectedBox'], formatter:'.createSelectedBoxDiv'}\" />\n                </l:VerticalLayout>\n            </m:ScrollContainer>\n\n            <m:ResponsivePopover\n                    id=\"popover\"\n                    placement=\"Right\"\n                    title=\"Assign Field\">\n                <m:content>\n                    <form:SimpleForm\n                            layout=\"ResponsiveGridLayout\"\n                            editable=\"true\"\n                            labelSpanS=\"3\"\n                            emptySpanS=\"0\"\n                            emptySpanM=\"0\">\n                        <m:Label text=\"Field\" required=\"true\"/>\n                        <m:ComboBox id=\"selectionPopoverFieldComboBox\"\n                                    items=\"{\n                                        path: '/extractionFields',\n                                        sorter: {\n                                            path: 'groupLabel',\n                                            descending: false,\n                                            group: true\n                                        }\n                                    }\"\n                                    selectedKey=\"{/extractionFieldInPopover/key}\"\n                                    change=\"handlePopoverChange\"\n                                    valueState= \"{= !${/extractionFieldInPopover/key} ? 'Error' : 'None'}\"\n                                    valueStateText= \"{= !${/extractionFieldInPopover/key} ? 'Select a field' : ' '}\">\n                            <Item key=\"{key}\" text=\"{label}\"\n                                  customdata:type=\"{type}\"\n                                  customdata:name=\"{name}\"\n                                  customdata:group=\"{group}\" />\n                        </m:ComboBox>\n                        <m:Label text=\"Value\" required=\"{= !${/isAnnotationUI}}\"/>\n                        <m:TextArea id=\"selectionPopoverValue\"\n                                    cols=\"25\"\n                                    growing=\"true\"\n                                    showExceededText=\"true\"\n                                    liveChange=\"handlePopoverChange\"/>\n                        <m:Label text=\"Row index\"/>\n                        <m:StepInput id=\"selectionPopoverRowIndex\"\n                                     value=\"{= ${/extractionFieldInPopover/rowIndex} + 1}\"\n                                     min=\"1\"\n                                     step=\"1\"\n                                     visible=\"false\"\n                                     textAlign=\"Center\"\n                                     change=\"handlePopoverChange\">\n                            <m:layoutData>\n                                <l:GridData span=\"XL1 L1 M1 S4\" />\n                            </m:layoutData>\n                        </m:StepInput>\n                    </form:SimpleForm>\n                </m:content>\n                <m:beginButton>\n                    <m:Button id=\"selectionPopoverApply\" text=\"Apply\" press=\"handlePopoverApply\" type=\"Emphasized\" enabled=\"false\"/>\n                </m:beginButton>\n                <m:endButton>\n                    <m:Button id=\"selectionPopoverCancel\" text=\"Cancel\" press=\"handlePopoverCancel\" />\n                </m:endButton>\n            </m:ResponsivePopover>\n            <m:Dialog id=\"selectorHelpPopover\"\n                type=\"Message\"\n                state=\"Information\"\n                title=\"How to annotate sample document?\"\n                icon=\"sap-icon://sys-help-2\"\n                contentWidth=\"600px\"\n                visible=\"false\">\n                <m:content>\n                    <m:Carousel arrowsPlacement=\"PageIndicator\">\n                        <m:customLayout>\n                            <m:CarouselLayout visiblePagesCount=\"1\" />\n                        </m:customLayout>\n                        <f:Card\n                            width=\"100%\">\n                            <f:header>\n                                <card:Header\n                                    title=\"Step 1. Select Area\"/>\n                            </f:header>\n                            <f:content>\n                                <m:VBox\n                                    class=\"sapUiSmallMargin\" width=\"100%\">\n                                    <m:Text text=\"Select the area you want to annotate. Or click the word you want to annotate.\" class=\"sapUiSmallMarginBottom\"/>\n                                    <m:Image width=\"100%\" src=\"staticcontent/images/select-area.gif\" />\n                                </m:VBox>\n                            </f:content>\n                        </f:Card>\n\n                        <f:Card\n                            width=\"100%\">\n                            <f:header>\n                                <card:Header\n                                    title=\"Step 2. Resize Selection\"/>\n                            </f:header>\n                            <f:content>\n                                <m:VBox\n                                    class=\"sapUiSmallMargin\" width=\"100%\">\n                                    <m:Text text=\"Enlarge or reduce the selection area if needed.\" class=\"sapUiSmallMarginBottom\"/>  \n                                    <m:Image width=\"100%\" src=\"staticcontent/images/resize-selection.gif\" />\n                                </m:VBox>\n                            </f:content>\n                        </f:Card>\n\n                        <f:Card\n                            width=\"100%\">\n                            <f:header>\n                                <card:Header\n                                    title=\"Step 3. Assign Field\"/>\n                            </f:header>\n                            <f:content>\n                                <m:VBox\n                                    class=\"sapUiSmallMargin\" width=\"100%\">\n                                    <m:Text text=\"Select the field name in the dropdown list. Add or change the value if needed. Click Apply.\" class=\"sapUiSmallMarginBottom\"/>  \n                                    <m:Image width=\"100%\" src=\"staticcontent/images/assign-field.gif\" />\n                                </m:VBox>\n                            </f:content>\n                        </f:Card>\n                    </m:Carousel>\n                </m:content>\n                <m:beginButton>\n                    <m:Button type=\"Emphasized\" text=\"OK\" press=\"handleHelpDialogClose\"></m:Button>\n                </m:beginButton>\n\n            </m:Dialog>\n        </m:items>\n    </m:VBox>\n</FragmentDefinition>"});
        module.exports = "sap.bdp.components.reuse.control.OCRViewer";